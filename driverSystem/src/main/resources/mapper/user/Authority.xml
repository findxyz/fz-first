<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.swj.model.user.Authority">
	<insert id="addAuthority" parameterType="Authority" useGeneratedKeys="true">
		insert into sys_authority(id,userid,roleid) values(#{id},#{userid},#{roleid})
	</insert>
</mapper>